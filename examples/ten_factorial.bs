; This program will calculate 10! and put it into a memory
; word called FACTORIAL. Print this number into the terminal
COPY 0x01 R0
COPY 0x0A R1
LOOP COPY 0x01 R2
MUL R1 R0
EQ 0x01 R1
JMP END
SUB R1 R2
COPY R2 R1
COPY 0x01 R2
JMP LOOP
END WRITE R0 FACTORIAL
READ FACTORIAL R0
WRITE R0 0xFFE2
HALT
FACTORIAL WORD 0x00